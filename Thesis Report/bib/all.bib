@article {Pappu2026,
	author = {Pappu, Ravikanth and Recht, Ben and Taylor, Jason and Gershenfeld, Neil},
	title = {Physical One-Way Functions},
	volume = {297},
	number = {5589},
	pages = {2026--2030},
	year = {2002},
	doi = {10.1126/science.1074376},
	publisher = {American Association for the Advancement of Science},
	abstract = {Modern cryptographic practice rests on the use of one-way functions, which are easy to evaluate but difficult to invert. Unfortunately, commonly used one-way functions are either based on unproven conjectures or have known vulnerabilities. We show that instead of relying on number theory, the mesoscopic physics of coherent transport through a disordered medium can be used to allocate and authenticate unique identifiers by physically reducing the medium{\textquoteright}s microstructure to a fixed-length string of binary digits. These physical one-way functions are inexpensive to fabricate, prohibitively difficult to duplicate, admit no compact mathematical representation, and are intrinsically tamper-resistant. We provide an authentication protocol based on the enormous address space that is a principal characteristic of physical one-way functions.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/297/5589/2026},
	eprint = {http://science.sciencemag.org/content/297/5589/2026.full.pdf},
	journal = {Science}
}
